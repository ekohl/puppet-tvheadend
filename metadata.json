{
  "name": "puppet-tvheadend",
  "version": "0.1.1-rc0",
  "author": "Vox Pupuli",
  "summary": "Puppet TVHeadend management module",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-tvheadend.git",
  "project_page": "http://github.com/voxpupuli/puppet-tvheadend",
  "issues_url": "https://github.com/voxpupuli/puppet-tvheadend/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.1 < 5.0.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 2.1.0 < 3.0.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 1.2.5 < 3.0.0"
    },
    {
      "name": "stm/debconf",
      "version_requirement": ">= 2.0.0 < 3.0.0"
    },
    {
      "name": "herculesteam/augeasproviders_core",
      "version_requirement": ">= 2.1.0 < 3.0.0"
    },
    {
      "name": "herculesteam/augeasproviders_shellvar",
      "version_requirement": ">= 2.2.1 < 3.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.7.1 < 6.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "16.04"
      ]
    }
  ]
}
